{
	"rewrites": [
		{ "source": "/@components/(.*)", "destination": "/src/components/$1" },
		{ "source": "/@pages/(.*)", "destination": "/src/pages/$1" },
		{ "source": "/@images/(.*)", "destination": "/src/images/$1" },
		{ "source": "/@redux/(.*)", "destination": "/src/redux/$1" },
		{ "source": "/@public/(.*)", "destination": "/public/$1" },
		{ "source": "/@(.*)", "destination": "/src/$1" }
	]
}
